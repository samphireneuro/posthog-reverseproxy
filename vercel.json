{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "nodeVersion": "20.x",
  "functions": {
    "api/klvyo-script.ts": {
      "runtime": "@vercel/node@4.0.0"
    }
  },
  "routes": [
    {
      "src": "/ph/static/(.*)",
      "dest": "https://eu.i.posthog.com/static/$1"
    },
    {
      "src": "/ph/(.*)",
      "dest": "https://eu.i.posthog.com/$1"
    },
    {
      "src": "/plsbl/js/script.js",
      "dest": "https://plausible.io/js/script.js"
    },
    {
      "src": "/plsbl/api/event",
      "dest": "https://plausible.io/api/event"
    },
    {
      "src": "/klvyo/script",
      "dest": "/api/klvyo-script"
    },
    {
      "src": "/klvyo/static/(.*)",
      "dest": "https://static-tracking.klaviyo.com/$1"
    },
    {
      "src": "/klvyo/api/(.*)",
      "dest": "https://a.klaviyo.com/api/$1"
    }
  ]
}
